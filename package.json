{
  "name": "frontend-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-toastify": "^7.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.3.1",
    "web-vitals": "^0.2.4"
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^7.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "react-hooks",
      "prettier"
    ],
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./config/webpack.config.prod.js"
        }
      },
      "react": {
        "version": "16.13.1"
      }
    },
    "rules": {
      "react-hooks/rules-of-hooks": 0,
      "react-hooks/exhaustive-deps": 0,
      "no-useless-escape": 0,
      "no-console": 0,
      "no-eval": 0,
      "consistent-return": 0,
      "array-callback-return": 0
    }
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
